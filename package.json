{
  "name": "html5-framework",
  "version": "1.0.0",
  "description": "html5-framework for web app",
  "main": "lib/index.js",
  "author": "RylanYan",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "6.7.4",
    "babel-runtime": "6.6.1",
    "bluebird": "3.3.5",
    "class-private-method-decorator": "^2.1.1",
    "classnames": "2.2.3",
    "core-decorators": "^0.12.3",
    "eventemitter3": "1.2.0",
    "fastclick": "1.0.6",
    "lodash": "4.11.1",
    "normalize.css": "4.1.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.5.2",
    "superagent": "1.4.0"
  },
  "devDependencies": {
    "asset": "^0.4.13",
    "babel-core": "^6.11.4",
    "babel-eslint": "6.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es5-property-mutators": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node5": "11.0.1",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
    "browser-sync": "^2.12.8",
    "browser-sync-webpack-plugin": "^1.0.1",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "file-exists": "^2.0.0",
    "file-loader": "^0.8.4",
    "file-system": "^2.2.1",
    "gaze": "^1.1.1",
    "ghooks": "^1.3.2",
    "jsonfile": "^2.3.1",
    "jsx-loader": "^0.13.2",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "object-recursive-iterator": "^1.0.2",
    "postcss-loader": "^0.9.1",
    "prompt": "^1.0.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-hmr": "^1.0.1",
    "recursive-readdir-sync": "^1.0.6",
    "source-map-support": "0.3.2",
    "string-format": "^0.5.0",
    "style-loader": "^0.13.1",
    "stylefmt": "^3.3.0",
    "stylelint": "^6.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.4.1",
    "yargs": "^4.6.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "",
      "commit-msg": ""
    }
  },
  "lint-staged": {
    "@(src|tools)/**/*.js": "eslint",
    "src/**/*.css": "stylelint"
  },
  "scripts": {
    "eslint": "eslint src tools",
    "stylelint": "stylefmt -R src && stylelint \"src/**/*.css\" --syntax css",
    "lint": "npm run eslint && npm run stylelint",
    "test": "mocha src/**/*.test.js --require test/setup.js --compilers js:babel-register",
    "test:watch": "mocha src/**/*.test.js --require test/setup.js --compilers js:babel-register --reporter min --watch",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "start": "babel-node tools/run start",
    "makecss": "babel-node tools/run makecss",
    "release": "babel-node tools/run build && babel ./src --out-dir ./lib",
    "compile": "babel-node tools/run compile",
    "init_project": "node init_project"
  }
}
